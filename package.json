{
  "name": "@zolyn/utils",
  "version": "1.0.0",
  "description": "Nothing, just utils.",
  "keywords": [
    "utils"
  ],
  "homepage": "https://github.com/Zolyn/utils#readme",
  "bugs": {
    "url": "https://github.com/Zolyn/utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zolyn/utils.git"
  },
  "license": "MIT",
  "author": "Yumeoto Zorin <zorin1477@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production tsup",
    "commit": "cz",
    "dev": "cross-env NODE_ENV=development tsup",
    "ensure": "git-ensure --no-behind -c",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint:fix": "nr lint --fix",
    "prepare": "husky install",
    "prepublishOnly": "nr build",
    "release": "nr ensure && bumpp --commit --tag --push"
  },
  "config": {
    "commitizen": {
      "path": "@zolyn/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@antfu/ni": "^0.16.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@zolyn/cz-conventional-changelog": "^1.0.1",
    "@zolyn/eslint-config": "^1.0.3",
    "bumpp": "^8.2.1",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "git-ensure": "^0.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "sort-package-json": "^1.57.0",
    "tsup": "^6.1.2",
    "tsx": "^3.4.3",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
